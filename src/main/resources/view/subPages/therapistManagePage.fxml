<AnchorPane prefHeight="610.0" prefWidth="1060.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.project.healthCareCenter.controller.TherapistManageController">
   <children>
      <ImageView fitHeight="610.0" fitWidth="1060.0" opacity="0.78" pickOnBounds="true">
         <image>
            <Image url="@../Imgs/bg.jpg" />
         </image>
      </ImageView>
      <Label layoutX="256.0" layoutY="166.0" prefHeight="27.0" prefWidth="176.0" text="Therpist ID            :" textFill="#243382">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <effect>
            <DropShadow>
               <color>
                  <Color red="1.0" green="1.0" blue="1.0" />
               </color>
            </DropShadow>
         </effect>
      </Label>
      <TextField fx:id="nameLabel" layoutX="456.0" layoutY="204.0" prefHeight="34.0" prefWidth="324.0" promptText="Justin Perera" style="-fx-border-width: 0 0 1px 0; -fx-border-color: blue; -fx-background-color: transparent;">
         <font>
            <Font size="14.0" />
         </font></TextField>
      <Label layoutX="254.0" layoutY="211.0" prefHeight="27.0" prefWidth="163.0" text="Patient Name        :" textFill="#243382">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <effect>
            <DropShadow>
               <color>
                  <Color red="1.0" green="1.0" blue="1.0" />
               </color>
            </DropShadow>
         </effect>
      </Label>
      <Label fx:id="idLabel" layoutX="456.0" layoutY="166.0" text="ID - 30030" textFill="#243382">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="saveBtn" layoutX="154.0" layoutY="381.0" mnemonicParsing="false" onAction="#saveOnClick" prefHeight="34.0" prefWidth="176.0" style="-fx-background-color: green; -fx-text-fill: white; -fx-font-weight: bold;" text="Save" />
      <Button layoutX="343.0" layoutY="381.0" mnemonicParsing="false" onAction="#updateOnClick" prefHeight="34.0" prefWidth="190.0" style="-fx-background-color: blue; -fx-text-fill: white; -fx-font-weight: bold;" text="Update" />
      <Button layoutX="549.0" layoutY="380.0" mnemonicParsing="false" onAction="#deleteOnClick" prefHeight="34.0" prefWidth="151.0" style="-fx-background-color: red; -fx-text-fill: white; -fx-font-weight: bold;" text="Delete" />
      <Button layoutX="717.0" layoutY="380.0" mnemonicParsing="false" onAction="#showTherapistTableOnClick" prefHeight="34.0" prefWidth="163.0" style="-fx-background-color: black; -fx-text-fill: white; -fx-font-weight: bold;" text="Show Therpists" />
      <Button fx:id="showProgramBtn" layoutX="453.0" layoutY="259.0" mnemonicParsing="false" onAction="#showProgramOnClick" prefHeight="34.0" prefWidth="310.0" style="-fx-background-color: black; -fx-text-fill: white; -fx-font-weight: bold;" text="Show Program" />
      <Label layoutX="256.0" layoutY="265.0" text="Therpist Program  :" textFill="#243382">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <effect>
            <DropShadow>
               <color>
                  <Color red="1.0" green="1.0" blue="1.0" />
               </color>
            </DropShadow>
         </effect>
      </Label>
